def squer(x,y,c):
    t.begin_fill()
    t.color(c)
    t.forward(x)
    t.left(90)
    t.forward(y)
    t.left(90)
    t.forward(x)
    t.left(90)
    t.forward(y)
    t.left(90)
    t.end_fill()

squer(40,20,"black")
t.penup()
t.goto(-600,180)
squer(40,20,"black")

t.penup()
t.goto(-600,140)
t.penup()
squer(40,20,"black")

t.penup()
t.goto(-600,100)
t.penup()
squer(40,20,"black")

t.penup()
t.goto(-600,60)
t.penup()
squer(40,20,"black")

t.penup()
t.goto(-600,20)
t.penup()
squer(40,20,"black")

t.penup()
t.goto(-530,20)
t.pendown()
squer(40,20,"black")

t.penup()
t.goto(-530,60)
t.pendown()
squer(40,20,"black")

t.penup()
t.goto(-530,100)
t.pendown()
squer(40,20,"black")

t.penup()
t.goto(-530,140)
t.pendown()
squer(40,20,"black")
