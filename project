#light
t.left(180)
t.penup()
t.goto(600,-30)
t.pendown()
t.left(90)

def light():
    t.color("#566573")
    t.forward(60)
    t.left(60)
    t.forward(30)
    t.color("#FFFF00")
    t.circle(2)


light()

t.penup()
t.goto(500,-30)
t.pendown()
t.right(60)
light()

t.penup()
t.goto(400,-30)
